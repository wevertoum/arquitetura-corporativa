    <element xsi:type="archimate:BusinessActor" name="Arquiteto corporativo" id="id-f69b2e37e1c94fb897d05bfd1aeb7856"/>
    <element xsi:type="archimate:BusinessActor" name="equipe de TI" id="id-08ae44a117144817abe73b46a5db0285"/>
    <element xsi:type="archimate:BusinessActor" name="implementador" id="id-651895e182cf4a16a0bbe7f83945cbb1"/>
